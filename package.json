{
  "name": "eve-profile",
  "displayName": "Eve Profile",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"electron . local\"",
    "build": "ng build -c production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package:win": "npm run build && cross-var electron-packager . EveProfile --out=packages --platform=win32 --arch=x64 --overwrite --asar",
    "package:linux": "npm run build && cross-var electron-packager . EveProfile --out=packages --platform=linux --arch=all --overwrite --asar",
    "package:osx": "npm run build && cross-var electron-packager . EveProfile --out=packages --platform=darwin --arch=all --overwrite --asar",
    "package:all": "npm run build && cross-var electron-packager . EveProfile --out=packages --all --arch=all --overwrite --asar"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "jszip": "^3.2.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.5",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.12.30",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "cross-var": "^1.1.0",
    "electron": "^4.0.8",
    "electron-packager": "^13.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
